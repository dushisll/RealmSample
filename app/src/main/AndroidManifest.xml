<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="dushisll.com.realmsample">

    <application
        android:name=".XApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.PersonListActivity"/>
        <activity android:name=".activity.SelectedPersonActivity"/>
        <activity android:name=".activity.QueryActivity"/>
        <activity android:name=".activity.UpdatePersonActivity"/>
        <activity android:name=".activity.ConditionQueryActivity"/>
        <activity android:name=".activity.OtherQueryActivity"/>
        <activity android:name=".activity.AsyncActivity"/>
        <activity android:name=".activity.AsyncAddActivity"/>
        <activity android:name=".activity.AsyncQueryActivity"/>
        <activity android:name=".activity.AsyncUpdateActivity"/>

    </application>

</manifest>